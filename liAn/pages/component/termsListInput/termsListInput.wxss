/* pages/component/termsListInput/termsListInput.wxss */

@import"../../../app.wxss";
.termsListInput {
    width: 100vw;
    /* background-color: red; */
    background: #fff;
}

.termsListInput>.termsListInput01 {
    margin: 0 auto;
    width: 690rpx;
    /* background-color: chartreuse; */
    display: flex;
    justify-content: space-between;
    align-items: center;
    height: 108rpx;
    flex-wrap: nowrap;
}

.termsListInput>.termsListInput01>.termsListInputTit {
    padding-left: 6rpx;
    font-size: 32rpx;
    font-family: PingFangSC, PingFangSC-Regular;
    font-weight: 400;
    text-align: left;
    color: #333333;
    line-height: 44rpx;
}

.termsListInput>.termsListInput01>.termsListInputIcon {
    padding-right: 18rpx;
    font-family: PingFangSC, PingFangSC-Regular;
    color: #333333;
    font-size: 18rpx;
}