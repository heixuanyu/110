/* pages/plan/plan.wxss */

.planContain01 {
    position: fixed;
    top: 0;
    bottom: 0;
    left: 0;
    background-color: yellowgreen;
    right: 0;
    height: 100vh;
    width: 100vw;
    z-index: 1;
}

.planContain02 {
    position: absolute;
    left: 50%;
    top: 70%;
    transform: translate(-50%, 0%);
    /* 50%为自身尺寸的一半 */
    height: 252rpx;
    width: 252rpx;
    z-index: 100;
    /* border: green 1px solid; */
}